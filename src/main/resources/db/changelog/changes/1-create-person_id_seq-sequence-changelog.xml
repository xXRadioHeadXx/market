<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
                   xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                                       http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.2.xsd
                                       http://www.liquibase.org/xml/ns/dbchangelog-ext
                                       http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd"
>

    <changeSet author="admin" id="1">
        <preConditions onFail="MARK_RAN">
            <not>
                <sequenceExists sequenceName="person_id_seq"/>
            </not>
        </preConditions>
        <createSequence cycle="false" incrementBy="1" maxValue="9223372036854775807" minValue="1" sequenceName="person_id_seq" startValue="1"/>
<!--        <createSequence cacheSize="1" cycle="false" dataType="bigint" incrementBy="1" maxValue="9223372036854775807" minValue="1" sequenceName="person_id_seq" startValue="1"/>-->
        <rollback>
            <dropSequence sequenceName="person_id_seq"/>
        </rollback>
    </changeSet>

</databaseChangeLog>